{
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [{ "key": "Cache-Control", "value": "no-store" }]
    }
  ],
  "rewrites": [
    { "source": "/icons/icon-192.png", "destination": "/icons/icon-192.svg" },
    { "source": "/icons/icon-512.png", "destination": "/icons/icon-512.svg" }
  ],
  "git": {
    "deploymentEnabled": {
      "preview": false,
      "production": true
    }
  },
  "ignoreCommand": "if [ \"$VERCEL_GIT_COMMIT_REF\" = \"ci/ignore-main-build\" ]; then exit 1; else exit 0; fi"
}
